{
  "name": "@mine/site",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "npm run clear && npm run route && next build",
    "clear": "rm -rf public/static && rm -rf .next",
    "create-app": "node ./scripts/create-app.mjs",
    "dev": "next dev",
    "dev:": "next dev",
    "export:analyze": "cross-env DEPLOY_ENV=local ANALYZE=true npm run build && next export -o ../../local-export/",
    "export:domain": "cross-env DEPLOY_ENV=domain npm run build && next export -o ../../domain-export/",
    "export:local": "cross-env DEPLOY_ENV=local npm run build && next export -o ../../local-export/",
    "file-type": "ts-node ./scripts/file-type.mjs",
    "lint:es": "eslint --ext .js,.ts,.tsx ./",
    "lint:es-fix": "npm run lint:es -- --fix",
    "qr": "ts-node ./scripts/qr.mjs",
    "route": "webpack-node ./scripts/route.ts ./scripts/tsconfig.route.json",
    "serve": "next start",
    "ts:check": "tsc --noEmit"
  },
  "browserslist": {
    "production": [
      "last 1 chrome version"
    ],
    "development": [
      "last 1 chrome version"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.10.8",
    "@emotion/styled": "^11.10.8",
    "@mui/icons-material": "^5.11.16",
    "@mui/lab": "^5.0.0-alpha.128",
    "@mui/material": "^5.12.2",
    "@mui/system": "^5.12.1",
    "@mui/x-date-pickers": "^6.3.0",
    "classnames": "^2.3.1",
    "dayjs": "^1.11.0",
    "disqus-react": "^1.1.3",
    "highlight.js": "^11.8.0",
    "html2canvas": "^1.4.0",
    "lodash": "^4.17.21",
    "mobx": "^6.9.0",
    "mobx-react-lite": "3.4.3",
    "next": "^13.3.1",
    "pinyin": "^2.11.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-image-crop": "^10.0.0",
    "react-markdown": "^8.0.0",
    "rehype-raw": "^6.1.0",
    "rehype-sanitize": "^5.0.0",
    "remark-gfm": "^3.0.1",
    "swiper": "^9.2.4",
    "three": "^0.152.0"
  },
  "devDependencies": {
    "@mine/markdown-loader": "^0.0.0",
    "@mine/webpack-node": "^0.0.0",
    "@next/bundle-analyzer": "^13.3.1",
    "@types/classnames": "^2.3.1",
    "@types/lodash": "^4.14.178",
    "@types/pinyin": "^2.10.0",
    "@types/react": "^18.2.0",
    "@types/react-test-renderer": "^18.0.0",
    "@types/three": "^0.150.0",
    "browserslist": "^4.21.4",
    "chalk": "^5.0.0",
    "cross-env": "^7.0.3",
    "file-loader": "^6.2.0",
    "fs-extra": "^11.1.0",
    "inquirer": "^9.2.0",
    "internal-ip": "^7.0.0",
    "log-symbols": "^5.1.0",
    "qrcode-terminal": "^0.12.0",
    "react-test-renderer": "^18.2.0",
    "sass": "^1.62.1",
    "tslib": "^2.5.0",
    "typescript": "^5.0.4",
    "webpack": "^5.81.0",
    "yaml-loader": "^0.8.0"
  }
}
