{
  "name": "@mine/site",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "npm run clear && npm run route && next build",
    "clear": "rm -rf public/static && rm -rf .next",
    "dev": "next dev",
    "export:domain": "cross-env DEPLOY_ENV=domain npm run build && next export -o ../../domain-export/",
    "export:local": "cross-env DEPLOY_ENV=local npm run build && next export -o ../../local-export/",
    "route": "webpack-node route.ts tsconfig.route.json ",
    "serve": "next start",
    "ts:check": "tsc --noEmit"
  },
  "browserslist": {
    "production": [
      "last 1 chrome version"
    ],
    "development": [
      "last 1 chrome version"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "@material-ui/system": "^4.12.1",
    "classnames": "^2.3.1",
    "dayjs": "^1.10.5",
    "disqus-react": "^1.1.1",
    "highlight.js": "^11.3.1",
    "mobx": "^6.3.2",
    "mobx-react-lite": "3.2.0",
    "next": "^11.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-markdown": "^6.0.2",
    "rehype-raw": "^5.1.0",
    "rehype-sanitize": "^4.0.0",
    "remark-gfm": "^1.0.0"
  },
  "devDependencies": {
    "@mine/markdown-loader": "^0.0.0",
    "@mine/webpack-node": "^0.0.0",
    "@types/classnames": "^2.3.1",
    "@types/react": "^17.0.13",
    "@types/react-test-renderer": "^17.0.1",
    "cross-env": "^7.0.3",
    "file-loader": "^6.2.0",
    "react-test-renderer": "^17.0.2",
    "sass": "^1.35.1",
    "typescript": "^4.3.5",
    "webpack": "^5.64.0",
    "yaml-loader": "^0.6.0"
  }
}
