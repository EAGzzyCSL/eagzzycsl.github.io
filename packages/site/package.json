{
  "name": "@mine/site",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "npm run clear && npm run route && next build",
    "clear": "rm -rf public/static && rm -rf .next",
    "dev": "next dev",
    "export:domain": "cross-env DEPLOY_ENV=domain npm run build && next export -o ../../domain-export/",
    "export:local": "cross-env DEPLOY_ENV=local npm run build && next export -o ../../local-export/",
    "route": "webpack-node route.ts tsconfig.route.json ",
    "serve": "next start",
    "ts:check": "tsc --noEmit"
  },
  "browserslist": {
    "production": [
      "last 1 chrome version"
    ],
    "development": [
      "last 1 chrome version"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@material-ui/system": "^4.11.2",
    "classnames": "^2.2.6",
    "dayjs": "^1.9.1",
    "disqus-react": "^1.0.11",
    "highlight.js": "^11.0.1",
    "mobx": "^6.0.4",
    "mobx-react-lite": "3.1.6",
    "next": "^10.0.3",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-markdown": "^5.0.3"
  },
  "devDependencies": {
    "@mine/markdown-loader": "^0.0.0",
    "@mine/webpack-node": "^0.0.0",
    "@types/classnames": "^2.2.10",
    "@types/react": "^17.0.0",
    "@types/react-test-renderer": "^17.0.0",
    "cross-env": "^7.0.2",
    "file-loader": "^6.1.0",
    "react-test-renderer": "^17.0.1",
    "sass": "^1.27.0",
    "typescript": "^4.0.2",
    "webpack": "^5.11.0"
  }
}
