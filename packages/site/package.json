{
  "name": "@mine/site",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "lint:es": "eslint --ext .js,.ts,.tsx ./",
    "lint:es-fix": "npm run lint:es -- --fix",
    "build": "npm run clear && npm run route && next build",
    "clear": "rm -rf public/static && rm -rf .next",
    "dev": "next dev",
    "export:domain": "cross-env DEPLOY_ENV=domain npm run build && next export -o ../../domain-export/",
    "export:local": "cross-env DEPLOY_ENV=local npm run build && next export -o ../../local-export/",
    "route": "webpack-node ./scripts/route.ts ./scripts/tsconfig.route.json",
    "create-app": "node ./scripts/create-app.mjs",
    "serve": "next start",
    "ts:check": "tsc --noEmit"
  },
  "browserslist": {
    "production": [
      "last 1 chrome version"
    ],
    "development": [
      "last 1 chrome version"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.4.4",
    "@mui/lab": "^5.0.0-alpha.82",
    "@mui/material": "^5.4.4",
    "@mui/styles": "^5.4.4",
    "@mui/system": "^5.7.0",
    "@mui/x-date-pickers": "^5.0.0-alpha.4",
    "classnames": "^2.3.1",
    "dayjs": "^1.11.0",
    "disqus-react": "^1.1.1",
    "highlight.js": "^11.5.1",
    "html2canvas": "^1.4.0",
    "lodash": "^4.17.21",
    "mobx": "^6.5.0",
    "mobx-react-lite": "3.4.0",
    "next": "^12.1.0",
    "pinyin": "^2.11.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-markdown": "^8.0.0",
    "rehype-raw": "^6.1.0",
    "rehype-sanitize": "^5.0.0",
    "remark-gfm": "^3.0.1"
  },
  "devDependencies": {
    "@mine/markdown-loader": "^0.0.0",
    "@mine/webpack-node": "^0.0.0",
    "@types/classnames": "^2.3.1",
    "@types/lodash": "^4.14.178",
    "@types/pinyin": "^2.10.0",
    "@types/react": "^17.0.13",
    "@types/react-test-renderer": "^17.0.1",
    "chalk": "^5.0.0",
    "cross-env": "^7.0.3",
    "file-loader": "^6.2.0",
    "fs-extra": "^10.1.0",
    "inquirer": "^8.2.0",
    "log-symbols": "^5.1.0",
    "react-test-renderer": "^17.0.2",
    "sass": "^1.49.9",
    "tslib": "^2.4.0",
    "typescript": "^4.6.2",
    "webpack": "^5.69.1",
    "yaml-loader": "^0.8.0"
  }
}
