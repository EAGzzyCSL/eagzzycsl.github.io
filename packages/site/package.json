{
  "name": "@mine/site",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "npm run clear && npm run route && next build",
    "build-storybook": "storybook build",
    "clear": "rm -rf public/static && rm -rf .next",
    "create-app": "node ./scripts/create-app.mjs",
    "dev": "next dev",
    "dev:": "next dev",
    "export:analyze": "cross-env DEPLOY_MODE=export DEPLOY_ENV=local ANALYZE=true npm run build",
    "export:domain": "cross-env DEPLOY_MODE=export DEPLOY_ENV=domain npm run build",
    "export:local": "cross-env DEPLOY_MODE=export DEPLOY_ENV=local npm run build",
    "file-type": "ts-node ./scripts/file-type.mjs",
    "lint:es": "eslint --ext .js,.mjs,.ts,.tsx ./",
    "lint:es-fix": "npm run lint:es -- --fix",
    "qr": "ts-node ./scripts/qr.mjs",
    "route": "rollup-node ./scripts/route.ts ./scripts/tsconfig.route.json",
    "serve": "next start",
    "storybook": "storybook dev -p 6006",
    "ts:check": "tsc --noEmit"
  },
  "browserslist": {
    "production": [
      "last 1 chrome version"
    ],
    "development": [
      "last 1 chrome version"
    ]
  },
  "dependencies": {
    "@chromatic-com/storybook": "^1.5.0",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mine/markdown-loader": "^0.0.0",
    "@mine/rollup-node": "^0.0.0",
    "@mui/icons-material": "^5.15.19",
    "@mui/lab": "^5.0.0-alpha.170",
    "@mui/material": "^5.15.19",
    "@mui/system": "^5.15.15",
    "@mui/x-date-pickers": "^7.6.2",
    "@next/bundle-analyzer": "^14.1.0",
    "@storybook/addon-essentials": "^8.1.6",
    "@storybook/addon-interactions": "^8.1.6",
    "@storybook/addon-links": "^8.1.6",
    "@storybook/addon-onboarding": "^8.1.6",
    "@storybook/blocks": "^8.1.6",
    "@storybook/nextjs": "^8.1.6",
    "@storybook/react": "^8.1.6",
    "@storybook/test": "^8.1.6",
    "@types/classnames": "^2.3.1",
    "@types/lodash": "^4.14.178",
    "@types/pinyin": "^2.10.0",
    "@types/react": "^18.2.0",
    "@types/react-test-renderer": "^18.0.0",
    "@types/three": "^0.161.2",
    "browserslist": "^4.21.9",
    "chalk": "^5.3.0",
    "classnames": "^2.3.1",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.0",
    "disqus-react": "^1.1.3",
    "eslint-plugin-storybook": "^0.8.0",
    "file-loader": "^6.2.0",
    "fs-extra": "^11.1.0",
    "highlight.js": "^11.8.0",
    "html2canvas": "^1.4.0",
    "inquirer": "^9.2.0",
    "internal-ip": "^8.0.0",
    "lodash": "^4.17.21",
    "log-symbols": "^6.0.0",
    "mobx": "^6.10.0",
    "mobx-react-lite": "4.0.3",
    "next": "^14.1.0",
    "pinyin": "^3.1.0",
    "qrcode-terminal": "^0.12.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-image-crop": "^11.0.1",
    "react-markdown": "^8.0.0",
    "react-test-renderer": "^18.2.0",
    "rehype-raw": "^6.1.0",
    "rehype-sanitize": "^5.0.0",
    "remark-gfm": "^3.0.1",
    "sass": "^1.70.0",
    "storybook": "^8.1.6",
    "swiper": "^11.1.4",
    "three": "^0.161.0",
    "tslib": "^2.6.2",
